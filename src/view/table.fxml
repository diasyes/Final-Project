          <TableColumn fx:id="stateCol" prefWidth="82.0" text="State" />
          <TableColumn fx:id="zipCol" prefWidth="160.0" text="Zip Code " />
            <TableColumn fx:id="latitudeCol" prefWidth="160.0" text="Latitude" />
